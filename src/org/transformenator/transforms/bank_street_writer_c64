; SOF
0080 = "{@@<FiLe_SoF>@@}"

; Two nulls is really EOF, but play it safe
00 =

; Hard space
8e = 20

; Emphasis of some kind
03 = 

; Emphasis
f4 =
f7 =
f9 =

; Transform capitals to lowercase
[41..5a] = 61

; Strip high bit from capitals
[c1..da] = 41