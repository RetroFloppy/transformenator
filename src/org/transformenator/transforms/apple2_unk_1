;
description = "Unknown Apple II word processor.  Newlines are 02 and 0d."
;

; Start of text
40030802 =

0040..08 =
00400302 = 0d0a
00400106 = 
004003 =
400102 =
400302 =

; Newline
02 = 0d0a
0d = 0d0a

; Spaces
a0 = 20
9a = 20
9b = 20
9c = 20
9d = 20
9e = 20
9f = 20

; Punctuation
[a1..af] = 21
[ba..bf] = 3a

; Numbers
[b0..b9] = 30

; Strip high bits from capitals
[c1..da] = 41

; Strip high bits from lower case
[e1..fa] = 61

; Strip off nulls
00 = 
ff =
